{
	"AurRpcUrl": "http://aur.olympus.local/",
	"AurTimeout": 5000,
	"AurSearchDelay": 500,
	"AurUseDifferentCommands": false,
	"AurInstallCommand": "",
	"AurUpgradeCommand": "",
